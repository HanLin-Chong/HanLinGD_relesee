<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.relesee.dao.UserDao">
    <select id="selectUserById" parameterType="java.lang.String" resultType="com.relesee.domains.User">
        SELECT
          username,password,userid,email,authority,userstate,phone,headphoto,deptid
        FROM
          users
        WHERE
          userid=#{userId}
    </select>


</mapper>